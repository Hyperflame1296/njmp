                },
                {
                    "controller.render.attack_indicator_progress_7": "v.is_first_person && q.has_property('njmp:player.attack_meter') && q.property('njmp:player.attack_meter') >= (7 / 16) && q.property('njmp:player.attack_meter') < (8 / 16)"
                },
                {
                    "controller.render.attack_indicator_progress_8": "v.is_first_person && q.has_property('njmp:player.attack_meter') && q.property('njmp:player.attack_meter') >= (8 / 16) && q.property('njmp:player.attack_meter') < (9 / 16)"
                },
                {
                    "controller.render.attack_indicator_progress_9": "v.is_first_person && q.has_property('njmp:player.attack_meter') && q.property('njmp:player.attack_meter') >= (9 / 16) && q.property('njmp:player.attack_meter') < (10 / 16)"
                },
                {
                    "controller.render.attack_indicator_progress_10": "v.is_first_person && q.has_property('njmp:player.attack_meter') && q.property('njmp:player.attack_meter') >= (10 / 16) && q.property('njmp:player.attack_meter') < (11 / 16)"
                },
                {
                    "controller.render.attack_indicator_progress_11": "v.is_first_person && q.has_property('njmp:player.attack_meter') && q.property('njmp:player.attack_meter') >= (11 / 16) && q.property('njmp:player.attack_meter') < (12 / 16)"
                },
                {
                    "controller.render.attack_indicator_progress_12": "v.is_first_person && q.has_property('njmp:player.attack_meter') && q.property('njmp:player.attack_meter') >= (12 / 16) && q.property('njmp:player.attack_meter') < (13 / 16)"
                },
                {
                    "controller.render.attack_indicator_progress_13": "v.is_first_person && q.has_property('njmp:player.attack_meter') && q.property('njmp:player.attack_meter') >= (13 / 16) && q.property('njmp:player.attack_meter') < (14 / 16)"
                },
                {
                    "controller.render.attack_indicator_progress_14": "v.is_first_person && q.has_property('njmp:player.attack_meter') && q.property('njmp:player.attack_meter') >= (14 / 16) && q.property('njmp:player.attack_meter') < (15 / 16)"
                },
                {
                    "controller.render.attack_indicator_progress_15": "v.is_first_person && q.has_property('njmp:player.attack_meter') && q.property('njmp:player.attack_meter') >= (15 / 16) && q.property('njmp:player.attack_meter') < 1.0"
                },
                {
                    "controller.render.attack_indicator_full": "v.is_first_person && q.has_property('njmp:player.attack_meter') && q.has_property('njmp:player.holding_weapon') && q.has_property('njmp:player.looking_at_entity') && q.property('njmp:player.holding_weapon') && q.property('njmp:player.looking_at_entity') && q.property('njmp:player.attack_meter') >= 1.0"
                }
